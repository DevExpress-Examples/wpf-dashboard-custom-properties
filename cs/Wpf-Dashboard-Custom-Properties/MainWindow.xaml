<Window xmlns:dxdash="http://schemas.devexpress.com/winfx/2008/xaml/dashboard"  x:Class="Wpf_Dashboard_Custom_Properties.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf_Dashboard_Custom_Properties"
        xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    
    <Grid>
        <dxdash:DashboardControl x:Name="dashboardControl">
            <dxmvvm:Interaction.Behaviors>
                <local:ChartItemModule/>
                <local:ItemDescriptionModule/>
                <local:DashboardDescriptionModule/>
            </dxmvvm:Interaction.Behaviors>
        </dxdash:DashboardControl>
    </Grid>
</Window>
